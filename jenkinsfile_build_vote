pipeline {
  agent { label 'master1' }
    stage('Build vote') {
      steps {
        sh 'docker build -t etzi71/vote:${env.BUILD_NUMBER} ./vote'
      }
    }
    stage('Push vote image') {
      steps {
        withDockerRegistry(credentialsId: 'dockerhub', url:'') {
          sh 'docker push etzi71/vote:${env.BUILD_NUMBER}'
        }
      }
    }
  }
}
